package UserInterface
public
	
	--implementing UserInterface
	with SEI;
	device UserInput
		features
			ui_out: out data port;
		properties 
			SEI::NetWeight=>0.7kg;
	end UserInput;
	
	device UserDisplay
		features
			ud_in: in data port;
		properties 
			SEI::NetWeight=>1.0kg;
	end UserDisplay;

end UserInterface;